#include <iostream>
using namespace std; 

void swap(int& a, int& b) {
    int temp = a;
    a = b;
    b = temp;
}

void sOrt(int colors[], int n) {
    int red = 0; 
    int blue = n - 1; 

    int i = 0;
    while (i <= blue) {
        if (colors[i] == 0) { 
            swap(colors[i], colors[red]);
            red++;
            i++;
        } else if (colors[i] == 2) { 
            swap(colors[i], colors[blue]);
            blue--;
        } else { 
            i++;
        }
    }
}

int main() {
    int n;
    cout << "Nhap so luong doi tuong: ";
    cin >> n;

    int colors[n];

	cout << "nhap cac phan tu(do=, trang =1, xanh =2)\n";
    for (int i = 0; i < n; ++i) {
        cin >> colors[i];
    }

    sOrt(colors, n);

    for (int i = 0; i < n; ++i) {
        if (colors[i] == 0)
            cout << "do" << " ";
        else if (colors[i] == 1)
            cout << "trang" << " ";
        else
            cout << "xanh" << " ";
    }

    return 0;
}
